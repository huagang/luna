<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
						http://code.alibabatech.com/schema/dubbo 
						http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
						default-lazy-init="true">

	<import resource="spring_config.xml" />
	<bean id="genNumService" class="ms.luna.biz.sc.mock.GenNumServiceImpl"/>
	<dubbo:application name="luna-service-impl" />

	<dubbo:registry protocol="zookeeper" address="${zookeeper.address}"/>
	<dubbo:protocol name="dubbo" port="20881" />
	<!-- 声明需要暴露的服务接口 -->
	<dubbo:consumer check="false" timeout="30000" retries="0" />

	<dubbo:service interface="ms.luna.biz.sc.AuthoritySetService" ref="authoritySetService" />
	<dubbo:service interface="ms.luna.biz.sc.CategoryService" ref="categoryService" />
	<dubbo:service interface="ms.luna.biz.sc.LoginService" ref="loginService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageAuthorityService" ref="manageAuthorityService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageBusinessService" ref="manageBusinessService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageMerchantService" ref="manageMerchantService" />
	<dubbo:service interface="ms.luna.biz.sc.ManagePoiService" ref="managePoiService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageShowAppService" ref="manageShowAppService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageUserService" ref="manageUserService" />
	<dubbo:service interface="ms.luna.biz.sc.MsShowPageService" ref="msShowPageService" />
	<dubbo:service interface="ms.luna.biz.sc.PulldownService" ref="pulldownService" />
	<dubbo:service interface="ms.luna.biz.sc.RegistService" ref="registService" />
	<dubbo:service interface="ms.luna.biz.sc.ShowPageComponentService" ref="showPageComponentService" />
	<dubbo:service interface="ms.luna.biz.sc.VodPlayService" ref="vodPlayService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageBusinessTreeService" ref="manageBusinessTreeService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageColumnService" ref="manageColumnService" />
	<dubbo:service interface="ms.luna.biz.sc.ManageArticleService" ref="manageArticleService" />
	<dubbo:service interface="ms.luna.biz.sc.PoiApiService" ref="poiApiService" />

</beans>